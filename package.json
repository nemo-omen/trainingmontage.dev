{
  "name": "sapper-postcss-template",
  "description": "A template that includes Sapper for Svelte and PostCSS preprocessing with Tailwind CSS",
  "keywords": [
    "sapper",
    "eslint",
    "postcss",
    "purgecss",
    "svelte",
    "tailwindcss",
    "cssnano"
  ],
  "homepage": "https://github.com/babichjacob/sapper-postcss-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/babichjacob/sapper-postcss-template.git"
  },
  "license": "MIT",
  "version": "2020.12.12",
  "scripts": {
    "eslint": "eslint",
    "eslint:fix": "eslint --fix ./*.js ./src/*.js ./src/routes/**/*.svelte ./src/components/**/*.svelte",
    "dev": "sapper dev --ext '.svelte .svx'",
    "build": "cross-env NODE_ENV=production sapper export --ext '.svelte .svx' --legacy",
    "prod": "npm run build",
    "export": "cross-env NODE_ENV=production sapper export --ext '.svelte .svx' --legacy",
    "start": "node __sapper__/build"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "d3-scale-chromatic": "^2.0.0",
    "express": "^4.17.1",
    "sirv": "^1.0.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "d3-array": "^2.9.1",
    "d3-axis": "^2.0.0",
    "d3-interpolate": "^2.0.1",
    "d3-path": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.0.0",
    "d3-time": "^2.0.0",
    "d3-time-format": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-svelte3": "^2.7.3",
    "gray-matter": "^4.0.2",
    "http-link-header": "^1.0.3",
    "kleur": "^4.1.3",
    "mdsvex": "^0.8.9",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.1",
    "postcss-import": "^13.0.0",
    "prism-svelte": "^0.4.7",
    "prismjs": "^1.22.0",
    "rehype-slug": "^4.0.1",
    "rehype-toc": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.10",
    "svelte": "^3.31.0",
    "svelte-feather-icons": "^3.3.0",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.2",
    "tailwindcss-grid-template": "^0.0.3",
    "twitter-lite": "^0.14.0"
  }
}